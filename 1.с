a = int(input())
b = int(input())
c = int(input())
if a < b + c and b < a + c and c < a + b:
    if a ** 2 == b ** 2 + c ** 2 or b ** 2 == a ** 2 + c ** 2 or c ** 2 == b ** 2 + b ** 2:
        print('right')
    elif a ** 2 < b ** 2 + c ** 2 and b ** 2 < a ** 2 + c ** 2 and c ** 2 < b ** 2 + b ** 2:
        print('acute')
    else:
        print('obtuse')
else:
    print('impossible')
